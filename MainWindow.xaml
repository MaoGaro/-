<Window x:Class="Прокат.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Прокат"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Height="140" VerticalAlignment="Center" Width="238">
            <Label Content="Логин"></Label>
            <TextBox x:Name="loginBox"></TextBox>
            <Label Content="Пароль"></Label>
            <TextBox x:Name="passwordBox"></TextBox>
            <Button x:Name="enterButton" Height="18" Content="Вход" Click="EnterButtonClick"/>
        </StackPanel>
    </Grid>
</Window>
